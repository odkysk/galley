{
  "name": "@galley/templates-basic",
  "version": "1.0.0",
  "type": "module",
  "description": "Basic template collection for Galley",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "import": "./dist/templates/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf dist .svelte-kit"
  },
  "peerDependencies": {
    "@galley/core": "^1.0.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@galley/core": "workspace:*",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  }
}